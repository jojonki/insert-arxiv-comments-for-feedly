{
    "name": "Insert arXiv commments for feedly",
    "version": "0.1",
    "description": "Insert a paper comment into feedly arXiv pages",
    "permissions": [
        "activeTab", 
		"*://*.arxiv.org/*",
		"https://feedly.com/*"
    ],
    "background": {
        "scripts": ["jquery-3.4.1.min.js", "background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": ["https://feedly.com/*"],
        "js": ["content.js"]
    }],
    "icons": {
        "16": "images/icon_128.png",
        "32": "images/icon_128.png",
        "48": "images/icon_128.png",
        "128": "images/icon_128.png"
    },
    "manifest_version": 2
}
