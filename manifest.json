{
    "name": "POP!",
    "version": "0.1",
    "description": "Copy current selected text and current URL",
    "permissions": [
        "activeTab", 
		"https://arxiv.org/",
        "<all_urls>",
        "tabs",
		"https://feedly.com/"
    ],
    "background": {
        "scripts": ["jquery-3.4.1.min.js", "background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "icons": {
    },
    "manifest_version": 2
}
